{
  "name": "@jscpd/sarif-reporter",
  "version": "3.5.10",
  "description": "sarif reporter for jscpd",
  "keywords": [
    "jscpd",
    "cpd",
    "reporter",
    "jscpd-reporter",
    "sarif"
  ],
  "author": "Dragos Varovici <dvarovici.work@gmail.com>",
  "homepage": "https://github.com/kucherenko/jscpd#readme",
  "license": "MIT",
  "main": "src/index",
  "types": "src/index",
  "directories": {
    "src": "src",
    "test": "__tests__"
  },
  "files": [
    "dist",
    "public"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kucherenko/jscpd.git"
  },
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rm -rf ./dist",
    "compile": "tsc -p tsconfig.build.json",
    "prepare": "ts-node ../../build-utils/publish-util.ts ./package.json dist/index",
    "postpublish": "ts-node ../../build-utils/publish-util.ts ./package.json src/index",
    "prepublishOnly": "yarn build"
  },
  "bugs": {
    "url": "https://github.com/kucherenko/jscpd/issues"
  },
  "dependencies": {
    "@jscpd/finder": "^3.5.10",
    "colors": "1.4.0",
    "fs-extra": "^9.0.1"
  },
  "devDependencies": {
    "@types/colors": "^1.2.1",
    "@types/fs-extra": "^8.1.0",
    "ajv": "^8.12.0",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^2.1.1",
    "autoprefixer": "^10.4.14",
    "install": "^0.13.0",
    "jscpd-badge-reporter": "^1.1.3",
    "json-schema-ref-parser": "^9.0.9",
    "mocha": "^7.1.2",
    "ncp": "^2.0.0",
    "ts-node": "^8.8.2"
  },
  "mocha": {
    "package": "./package.json",
    "extension": [
      "ts"
    ],
    "spec": [
      "**/*.test.ts"
    ],
    "recursive": true,
    "file": [],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "ui": "bdd",
    "exit": true
  },
  "gitHead": "87a118f80343288c0099a4e3e47ccd875476bf6c"
}
