{
  "name": "@jscpd/finder",
  "version": "3.6.0",
  "description": "detector of copy/paste in files",
  "author": "Andrey Kucherenko <kucherenko.andrey@gmail.com>",
  "homepage": "https://github.com/kucherenko/jscpd#readme",
  "license": "MIT",
  "type": "module",
  "main": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json"
  },
  "types": "src/index",
  "directories": {
    "src": "src",
    "test": "__tests__"
  },
  "files": [
    "dist",
    "html"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kucherenko/jscpd.git"
  },
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rm -rf ./dist",
    "compile": "tsc -p tsconfig.build.json",
    "prepare": "ts-node ../../build-utils/publish-util.ts ./package.json dist/index",
    "test": "nyc yarn run test:unit",
    "test:unit": "mocha -r source-map-support/register -r ts-node/register __tests__/**/*.test.ts",
    "postpublish": "ts-node ../../build-utils/publish-util.ts ./package.json src/index",
    "prepublishOnly": "yarn build"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@jscpd/core": "workspace:^",
    "@jscpd/tokenizer": "workspace:^",
    "blamer": "^1.0.6",
    "bytes": "^3.1.2",
    "cli-table3": "^0.6.3",
    "colors": "1.4.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "markdown-table": "^3.0.3",
    "pug": "^3.0.2"
  },
  "devDependencies": {
    "@types/bytes": "^3.1.4",
    "@types/fs-extra": "^11.0.4",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.5",
    "@types/pug": "^2.0.10",
    "chai": "^4.3.10",
    "mocha": "^10.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "gitHead": "fa06f839d6c5335a1e9dbf0c7387710fcd42eb54"
}
